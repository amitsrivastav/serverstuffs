{spawn, exec}  = require 'child_process'module.exports = (robot) -> robot.respond /get the list of indexes from elasticsearch cluster/i, (msg) ->    c = "/home/bot/mybot/scripts/bin/elasticsearch/es_tool.py --list-all-indices"    exec c, (err, stdout, stderr) ->      msg.send stdout      msg.send stderr      msg.send err  robot.respond /delete the index ([^ ]+)\s*(\w*) from elasticsearch cluster/i, (msg) ->    c = "/home/bot/mybot/scripts/bin/elasticsearch/es_tool.py --delete-index "+ msg.match[1]    exec c, (err, stdout, stderr) ->      msg.send stdout      msg.send stderr      msg.send err
